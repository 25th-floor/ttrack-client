{
  "name": "ttrack-client",
  "version": "0.5.0",
  "description": "Another TimeTracking Tool - Client Side",
  "author": "25th floor",
  "license": "MIT",
  "engines": {
    "yarn": ">= 0.27.5",
    "node": ">= 8.5.0"
  },
  "quokka": {
    "babel": {
      "presets": [
        "babel-preset-react-app"
      ],
      "path": "./node_modules/babel-core"
    }
  },
  "scripts": {
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "gen": "plop --plopfile generators/plopfile.js",
    "start": "node scripts/start.js",
    "https": "HTTPS=true node scripts/start.js",
    "build": "node scripts/build.js",
    "precommit": "lint-staged",
    "eslint": "eslint src",
    "flow": "flow",
    "jest": "jest",
    "test": "node scripts/test.js --env=jsdom"
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "dependencies": {
    "bootflat": "^2.0.4",
    "bootstrap": "~3.3.7",
    "promise": "8.0.1",
    "ramda": "^0.24.1",
    "react": "^16.0.0",
    "react-dev-utils": "^4.1.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-styleguidist": "git://github.com/25th-floor/react-styleguidist.git#091dde8",
    "redux": "^3.7.2",
    "redux-persist": "^4.10.1",
    "rest": "^2.0.0",
    "wfk-roboto": "^1.0.7"
  },
  "devDependencies": {
    "autoprefixer": "7.1.2",
    "babel-core": "6.25.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-preset-react-app": "^3.0.3",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "class-set": "0.0.4",
    "classnames": "^2.2.5",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "enzyme-to-json": "^3.1.4",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "flow-bin": "^0.56.0",
    "flow-remove-types": "^1.2.3",
    "font-awesome": "^4.7.0",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "inquirer-directory": "^2.1.0",
    "jasmine-check": "^1.0.0-rc.0",
    "jest": "20.0.4",
    "lint-staged": "^4.2.3",
    "lodash": "^4.17.4",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.18.1",
    "moment-duration-format": "^1.3.0",
    "object-assign": "4.1.1",
    "plop": "^1.8.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "postcss-modules": "^1.1.0",
    "prettier-eslint": "^8.2.0",
    "prettier-eslint-cli": "^4.4.0",
    "react-hot-loader": "next",
    "react-test-renderer": "16",
    "redux-logger": "^3.0.6",
    "remote-redux-devtools": "^0.5.12",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "testcheck": "^1.0.0-rc.2",
    "url-loader": "0.5.9",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.8.2",
    "webpack-manifest-plugin": "1.2.1",
    "whatwg-fetch": "2.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "verbose": true,
    "setupFiles": [
      "<rootDir>/config/rafPolyfill.js",
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/setupTest.js",
      "<rootDir>/config/localStorageMock.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/node_modules/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)",
      "<rootDir>/src/node_modules/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/(node_modules)/"
    ],
    "transformIgnorePatterns": [
      "^(?!.*([/\\\\]src[/\\\\]node_modules[/\\\\])).*([/\\\\]node_modules[/\\\\])"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/node_modules/$1",
      "^react-native$": "react-native-web",
      "\\module.css$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "css"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
