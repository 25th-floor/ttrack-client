{
  "name": "ttrack-client",
  "version": "0.4.0",
  "description": "Another TimeTracking Tool - Client Side",
  "main": "src/client/app.js",
  "directories": {
    "bin": "src"
  },
  "scripts": {
    "build": "webpack --config webpack.production.config.js",
    "dev": "webpack-dev-server",
    "lint": "eslint ./src",
    "fix-lint": "eslint --fix ./src",
    "flow": "flow check",
    "test": "jest --no-cache src/common/**/*"
  },
  "author": "25th floor",
  "license": "MIT",
  "engines": {
    "yarn": ">= 0.18.0"
  },
  "jest": {
    "collectCoverage": true,
    "setupTestFrameworkScriptFile": "./setup-jasmine-env.js",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/babel-runtime",
      "<rootDir>/node_modules/classnames",
      "<rootDir>/node_modules/core-js",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/immutable",
      "<rootDir>/node_modules/jasmine",
      "<rootDir>/node_modules/jasmine-check",
      "<rootDir>/node_modules/lodash",
      "<rootDir>/node_modules/moment",
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/testcheck"
    ],
    "roots": [
      "./src"
    ]
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "~6.24",
    "babel-eslint": "~7.2",
    "babel-jest": "~20.0",
    "babel-loader": "~7.0",
    "babel-plugin-react-intl": "~2.3",
    "babel-plugin-transform-class-properties": "~6.24",
    "babel-plugin-transform-function-bind": "~6.22",
    "babel-plugin-transform-object-rest-spread": "~6.23",
    "babel-preset-es2015": "~6.24",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "~6.24",
    "babel-preset-stage-2": "^6.24.1",
    "copy-webpack-plugin": "~4.0",
    "css-loader": "^0.28.2",
    "eslint": "~3.19",
    "eslint-config-airbnb": "15.0.1",
    "eslint-loader": "~1.7",
    "eslint-plugin-flowtype": "^2.25.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "~7.0",
    "extract-text-webpack-plugin": "~2.1",
    "file-loader": "^0.11.1",
    "flow-bin": "^0.46.0",
    "istanbul": "^0.4.5",
    "jasmine": "~2.6",
    "jasmine-check": "git://github.com/dready/jasmine-check.git#master",
    "jasmine-expect": "~3.7",
    "jasmine-expect-moment": "~0.1",
    "jasmine-expect-react": "~0.1",
    "jasmine-reporters": "~2.2",
    "jest-cli": "~20.0",
    "jsx-loader": "^0.13.2",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "path": "^0.12.7",
    "ramda": "^0.24.1",
    "react-addons-pure-render-mixin": "~15.5",
    "react-addons-test-utils": "~15.5",
    "react-hot-loader": "next",
    "style-loader": "^0.18.0",
    "url-loader": "^0.5.7",
    "webpack": "~2.6",
    "webpack-dev-server": "~2.4"
  },
  "dependencies": {
    "babel-polyfill": "~6.23",
    "bootflat": "^2.0.4",
    "bootstrap": "~3.3.7",
    "class-set": "0.0.4",
    "font-awesome": "4.7.0",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "moment": "~2.18.1",
    "moment-duration-format": "^1.3.0",
    "myro": "^0.6.3-alpha.0",
    "react": "~15.5",
    "react-dom": "~15.5",
    "rest": "^2.0.0"
  }
}
